.drop-content {
    width: 100%;
}

    .drop-content .select {
        width: 165px;
        height: 40px;
        background: rgba(0,0,0,.2);
        margin: 0px auto;
        position: relative;
        cursor: pointer;
    }

        .drop-content .select::after {
            content: "";
            display: block;
            width: 10px;
            height: 10px;
            border-left: 1px solid #D0D0D0;
            border-bottom: 1px solid #D0D0D0;
            top: 12px;
            right: 12px;
            position: absolute;
            transform: rotate(-46deg);
            transition: all .3s ease-in;
        }

        .drop-content .select p {
            width: 135px;
            overflow: hidden;
            text-overflow: ellipsis;
            line-height: 40px;
            font-size: 14px;
            color: #fff;
            padding-left: 30px;
            border-radius: 0 6px 6px 0;
            background: url(images/icon-top-pin.png) no-repeat 10px center;
        }

        .drop-content .select ul {
            width: 165px;
            display: block;
            font-size: 16px;
            background: #fff;
            position: absolute;
            top: 40px;
            left: 0px;
            max-height: 0px;
            overflow: hidden;
            transition: max-height .3s ease-in;
            border-radius: 0 0 6px 6px;
        }

            .drop-content .select ul li {
                height: 30px;
                line-height: 30px;
                padding: 0px 15px;
                list-style: none;
                color: #666666;
            }

                .drop-content .select ul li.selected {
                    background: #04739c;
                    color: #fff;
                }

                .drop-content .select ul li:hover {
                    background: #d0d0d0;
                }

@-webkit-keyframes slide-down {
    0% {
        transform: scale(1,0);
    }

    25% {
        transform: scale(1,1);
    }

    50% {
        transform: scale(1,0.85);
    }

    75% {
        transform: scale(1,1.02);
    }

    100% {
        transform: scale(1,1);
    }
}

.drop-content .select.open ul {
    max-height: 290px;
    transform-origin: 50% 0;
    -webkit-animation: slide-down .5s ease-in;
    transition: max-height .2s ease-in;
}

.drop-content .select.open::after {
    transform: rotate(134deg);
    transition: all .3s ease-in;
    top: 18px;
}
