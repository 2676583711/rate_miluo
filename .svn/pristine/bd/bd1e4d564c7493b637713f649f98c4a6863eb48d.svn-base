@charset "utf-8";

.clear {
    clear: both;
}

.clearfix:after {
    display: table;
    content: '';
    height: 0;
    clear: both;
}

.pull-left {
    float: left;
}

.pull-right {
    float: right;
}

.container {
    width: 100%;
}

/*头部开始*/
.top {
    position: fixed;
    top: 0px;
    left: 0px;
    right: 0px;
    height: 108px;
    background: #035588 url(images/top-bg.jpg) no-repeat left top;
    border-bottom: solid 1px #0081bb;
    z-index: 999;
    min-width: 1213px;
    -moz-box-shadow: 3px 0 6px 0px rgba(6, 0, 1, 0.77); /* FF3.5+ */
    -webkit-box-shadow: 3px 0 6px 0px rgba(6, 0, 1, 0.77); /* Saf3.0+, Chrome */
    box-shadow: 3px 0 6px 0px rgba(6, 0, 1, 0.77); /* Opera 10.5, IE 9.0 */
}



.logo {
    float: left;
}

    .logo a {
        display: inline-block;
        padding-top: 15px;
        padding-left: 15px;
    }

.info {
    position: absolute;
    top: 17px;
    right: 10px;
    text-align: right;
}

    .info li {
        display: inline-block;
    }

    .info a {
        position: relative;
        display: inline-block;
        padding: 5px 12px;
        color: #fff;
        text-align: center;
        text-decoration: none;
        border-radius: 3px;
    }

        .info a img {
            vertical-align: middle;
        }

        .info a:hover {
            background-color: rgba(255,255,255,.3);
        }

.top-right {
    position: absolute;
    top: 3px;
    right: 200px;
    width: 306px;
}

    .top-right > a {
        position: relative;
        display: block;
        float: left;
        width: 97px;
        padding-top: 42px;
        padding-bottom: 5px;
        color: #fff;
        text-align: center;
        text-decoration: none;
        background: url(images/top-jinji.png) no-repeat center 5px;
    }

        .top-right > a:last-child {
            border-right: 1px solid rgba(255,255,255,.1);
        }

        .top-right > a:hover {
            border-radius: 3px;
            background-color: rgba(255,255,255,.3);
        }

        .top-right > a.top-zhongdeng {
            background-image: url(images/top-zhongdeng.png);
        }

        .top-right > a.top-yiban {
            background-image: url(images/top-yiban.png);
        }


.top-label {
    position: absolute;
    top: 4px;
    right: 28px;
    display: block;
    width: 22px;
    height: 22px;
    line-height: 22px;
    color: #fff;
    text-align: center;
    border-radius: 50%;
    box-shadow: 0 0 10px rgba(0,0,0,.35);
}


.open > .dropdown-menu {
    display: block !important;
}
/*头部结束*/

/*水平导航 s*/
.h-nav {
    position: absolute;
    left: 0;
    right: 0;
    bottom: 0;
    height: 38px;
    line-height: 38px;
    background-color: #054a6e;
    border-top: 1px solid #3280ad;
}


ul.top-dropdown {
    position: relative;
    width: 100%;
}

    ul.top-dropdown > li {
        position: relative;
        float: left;
    }

    ul.top-dropdown > a:hover {
        color: #000;
    }

    ul.top-dropdown > li {
        width: 201px;
        height: 38px;
        line-height: 38px;
        margin-left: -31px;
        background: url(images/nav-bg.png) no-repeat;
    }

        ul.top-dropdown > li > a {
            position: relative;
            display: block;
            font-size: 16px;
            color: #fff;
            z-index: 2000;
            text-align: center;
            text-decoration: none;
        }



            ul.top-dropdown > li > a > img {
                margin-top: -3px;
                margin-right: 5px;
            }

        ul.top-dropdown > li.hover > a {
            background: url(images/nav-bg-active.png) no-repeat;
        }

        ul.top-dropdown > li:hover, ul.top-dropdown > li.hover {
            position: relative;
            background: url(images/nav-bg-active.png) no-repeat;
        }

    ul.top-dropdown ul {
        display: none;
        position: absolute;
        top: 0;
        left: 0;
        width: 170px;
        margin-left: 28px;
        z-index: 1000;
        border-radius: 0 5px 5px 0;
    }

        ul.top-dropdown ul li {
            font-weight: normal;
            background: #f6f6f6;
            color: #000;
            width:280px;
            border-bottom: 1px solid #ccc;
            box-shadow: 0 2px 8px rgba(0,0,0,.3);
        }

            ul.top-dropdown ul li a {
                display: block;
                padding-left: 10px;
                font-size: 14px;
                color: #34495e !important;
                background: #eee !important;
            }

            ul.top-dropdown ul li:last-child > a, ul.top-dropdown ul li:last-child {
                border-radius: 0 0 5px 5px;
            }

            ul.top-dropdown ul li a:hover {
                display: block;
                background: #387DA1 !important;
                color: #fff !important;
            }

.drop {
    transition: all .2s;
}

    .drop > a {
        position: relative;
    }

        .drop > a:after {
            content: "";
            position: absolute;
            right: 35px;
            top: 44%;
            border-left: 5px solid transparent;
            border-top: 5px solid #fff;
            border-right: 5px solid transparent;
            z-index: 999;
        }

        .drop > a:hover:after {
            content: "";
            border-left: 5px solid transparent;
            border-top: 5px solid #fff;
            border-right: 5px solid transparent;
        }
/*水平导航 e*/




/*导航开始*/
.navigation {
    position: absolute;
    top: 48px;
    bottom: 0px;
    left: 0px;
    z-index: 10;
    width: 212px;
    background: #1e97ce;
    font-size: 14px;
    border-right: solid 1px #0888c3;
    -moz-box-shadow: 2px 0 2px 0px #bababa; /* FF3.5+ */
    -webkit-box-shadow: 2px 0 2px 0px #bababa; /* Saf3.0+, Chrome */
    box-shadow: 2px 0 2px 0px #bababa; /* Opera 10.5, IE 9.0 */
}

.navigation-main {
}

.navigation-title {
    border-bottom: 1px solid #004362;
    height: 40px;
    line-height: 40px;
    font-size: 14px;
    font-weight: bold;
    color: #fff;
    background: url(images/icon-menu.png) no-repeat 12px center #006493;
    padding-left: 40px;
}

.navigation-content {
    position: absolute;
    top: 41px;
    bottom: 0;
    left: 0;
    right: 0;
    overflow: auto;
}

.collapse {
    position: relative;
    height: 0;
    overflow: hidden;
    -webkit-transition: height 0.35s ease;
    -moz-transition: height 0.35s ease;
    -o-transition: height 0.35s ease;
    transition: height 0.35s ease;
}

    .collapse.in {
        height: auto;
        transition: height 0.35s ease;
    }

.navigation-content li a.nav-1 {
    position: relative;
    display: block;
    height: 36px;
    line-height: 36px;
    padding-left: 40px;
    color: #fff;
    font-weight: bold;
    border-bottom: solid 1px #006493;
    background-color: #1b88bb;
    *background-color: #1b88bb;
    background-image: -moz-linear-gradient(top, #1da4e1, #1b88bb);
    background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#1da4e1), to(#1b88bb));
    background-image: -webkit-linear-gradient(top, #1da4e1, #1b88bb);
    background-image: -o-linear-gradient(top, #1da4e1, #1b88bb);
    background-image: linear-gradient(to bottom, #1da4e1, #1b88bb);
    background-repeat: repeat-y;
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#FF1da4e1', endColorstr='#FF1b88bb', GradientType=0);
    filter: progid:DXImageTransform.Microsoft.gradient(enabled=false);
}

    .navigation-content li a.nav-1:hover {
        background-color: #37bffc;
        *background-color: #37bffc;
        background-image: -moz-linear-gradient(top, #4ec8ff, #2D9BCE);
        background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#4ec8ff), to(#2D9BCE));
        background-image: -webkit-linear-gradient(top, #4ec8ff, #2D9BCE);
        background-image: -o-linear-gradient(top, #4ec8ff, #2D9BCE);
        background-image: linear-gradient(to bottom, #4ec8ff, #2D9BCE);
        background-repeat: repeat-y;
        filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#FF4ec8ff', endColorstr='#FF2D9BCE', GradientType=0);
        filter: progid:DXImageTransform.Microsoft.gradient(enabled=false);
    }

    .navigation-content li a.nav-1 img {
        position: absolute;
        left: 9px;
        top: 6px;
    }

    .navigation-content li a.nav-1 span {
        position: absolute;
        right: 12px;
        top: 15px;
        display: block;
        width: 9px;
        height: 9px;
        background: url(images/icon-unfold.png) no-repeat center;
    }

.navigation-content li.in a {
    background-color: #37bffc;
    *background-color: #37bffc;
    background-image: -moz-linear-gradient(top, #4ec8ff, #37bffc);
    background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#4ec8ff), to(#37bffc));
    background-image: -webkit-linear-gradient(top, #4ec8ff, #37bffc);
    background-image: -o-linear-gradient(top, #4ec8ff, #37bffc);
    background-image: linear-gradient(to bottom, #4ec8ff, #37bffc);
    background-repeat: repeat-y;
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#FF4ec8ff', endColorstr='#FF37bffc', GradientType=0);
    filter: progid:DXImageTransform.Microsoft.gradient(enabled=false);
}

    .navigation-content li.in a span {
        position: absolute;
        right: 12px;
        top: 15px;
        display: block;
        width: 9px;
        height: 9px;
        background: url(images/icon-fold.png) no-repeat center;
    }

.navigation-content li a.nav-1:hover span {
    background: url(images/icon-unfold-hov.png) no-repeat center;
}

.navigation-content li.in a.nav-1 span {
    background: url(images/icon-fold-hov.png) no-repeat center;
}

.navigation-content li.in a.nav-1:hover span {
