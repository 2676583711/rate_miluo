/*分页开始*/
.data-pager {
    float: right;
    padding: 10px 0 10px 0;
}

.data-pager-left {
    float: left;
}

    .data-pager-left li {
        float: left;
    }

        .data-pager-left li a {
            border: 1px solid #d1d6d8;
            border-left: none;
            display: block;
            padding: 5px 10px;
            text-align: center;
            color: #333;
        }

            .data-pager-left li a:hover {
                cursor: pointer;
                color: #0c9fcd;
                background: #f6f8f9;
            }

            .data-pager-left li a.active {
                color: #0c9fcd;
                font-weight: bold;
                background: #f6f8f9;
            }

            .data-pager-left li a img {
                margin-right: 5px;
                margin-left: 5px;
            }

        .data-pager-left li:first-child a {
            border-left: 1px solid #d1d6d8;
        }

        .data-pager-left li.data-pager-left-number {
            padding: 5px 10px;
        }

.data-pager-right {
    float: left;
    padding-top: 1px;
}

    .data-pager-right ul, .data-pager-left ul {
        float: left;
    }

    .data-pager-right li {
        float: left;
    }

        .data-pager-right li a {
            border: 1px solid #d1d6d8;
            border-right: none;
            display: block;
            padding: 3px 6px;
            text-align: center;
            color: #333;
        }

        .data-pager-right li:last-child a {
            border-right: 1px solid #d1d6d8;
        }

        .data-pager-right li a:hover {
            color: #0c9fcd;
            background: #f6f8f9;
        }

        .data-pager-right li a.active {
            color: #0c9fcd;
            font-weight: bold;
            background: #f6f8f9;
        }

        .data-pager-right li.second-child a {
            border-left: 1px solid #d1d6d8;
        }

.data-pager-right-number, .data-pager-left-number {
    display: inline-block;
    float: left;
    padding: 4px 6px;
    line-height: 1.6;
}
/*分页结束*/



/*模态弹出框*/
.modal {
    position: fixed;
    top: 40%;
    right: 0;
    bottom: 0;
    left: 0;
    z-index: 1050;
    display: none;
    overflow: hidden;
    -webkit-overflow-scrolling: touch;
    outline: 0;
}

.modal-dialog {
    position: absolute;
    top: 50%;
    left: 50%;
    margin-left: -150px;
    margin-top: -100px;
    width: 300px;
    height: 200px;
    z-index: 10002;
    display: none;
}

.modal-dialog-nofooter {
    margin-left: -140px;
    margin-top: -90px;
    width: 280px;
    height: 180px;
    z-index: 10002;
    display: none;
}

    .modal-dialog-nofooter .modal-body {
        bottom: 0;
    }

.modal-content {
    position: relative;
    background-color: #fff;
    -webkit-background-clip: padding-box;
    background-clip: padding-box;
    border-radius: 4px 4px 0 0;
    outline: 0;
    border: 2px solid #1E97CE;
    border-top: none;
    -webkit-box-shadow: 0 3px 9px rgba(0,0,0,.5);
    box-shadow: 0 3px 9px rgba(0,0,0,.5);
    height: 100%;
}


.modal-body-left {
    float: left;
    width: 25%;
    height: 99%;
    overflow: auto;
}

.modal-body-right {
    float: right;
    width: 74.8%;
    height: 99%;
    overflow: auto;
    border-left: 1px solid #d8dbe0;
}

    .modal-body-right .search {
        border-bottom: 1px solid #d8dbe0;
        padding-top: 4px;
    }

        .modal-body-right .search .contrl {
            margin-bottom: 5px;
        }

        .modal-body-right .search .input {
            width: 85px;
        }

        .modal-body-right .search .select {
            width: 97px;
        }

.loading {
    position: absolute;
    top: 50%;
    left: 50%;
    margin-top: -16px;
    margin-left: -16px;
}

.modal-header {
    padding: 8px 7px 8px 15px;
    background: #1E97CE;
    border-radius: 3px 3px 0 0;
}

.modal-title {
    font-size: 12px;
    color: #fff;
    font-weight: bold;
    margin: 0;
}

.modal-body {
    position: absolute;
    top: 33px;
    left: 0;
    bottom: 0;
    width: 100%;
    overflow: auto;
}

.modal-footer {
    position: absolute;
    left: 0;
    bottom: 0;
    right: 0;
    padding: 5px 10px;
    text-align: right;
    border-top: 1px solid #e5e5e5;
}

.modal-backdrop {
    position: fixed;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    z-index: 1040;
    background-color: #000;
}

    .modal-backdrop.fade.in {
        filter: alpha(opacity=30);
        opacity: .3;
    }

.modal-big {
    top: 48px;
    overflow: auto;
}

.fade {
    opacity: 0;
    -webkit-transition: opacity .15s linear;
    -o-transition: opacity .15s linear;
    transition: opacity .15s linear;
}

    .fade.in {
        opacity: 1;
    }


.modal-body .alert-tips {
    padding: 20px;
    text-align: center;
    margin-top: 25px;
    font-size: 14px;
}

    .modal-body .alert-tips img {
        vertical-align: middle;
        width: 42px;
        margin-right: 5px;
    }
/*模态弹出框 end*/

