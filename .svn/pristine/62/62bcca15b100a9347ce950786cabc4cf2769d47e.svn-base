	height: 410px;
}

.aqiDialogHeader {
	background: rgba(255, 255, 255, 0.3);
	text-align: center;
	width: 100%;
	height: 56px;
}

.aqiDialogHeader .title {
	padding-top: 5px;
	font-size: 18px;
	line-height: 24px;
	width: 100%;
	height: 24px;
}

.aqiDialogHeader .content {
	font-size: 14px;
	line-height: 24px;
	width: 100%;
	height: 24px;
}

.aqiDialogNeck {
	background: rgba(255, 255, 255, 0);
	text-align: center;
	font-family: Microsoft YaHei,tahoma,serif,simsun,fantasy;
	color: #FFFFFF;
	width: 100%;
	height: 80px;
}

.aqiDialogNeck .cityName {
	padding-top: 4px;
	padding-bottom: 4px;
	font-size: 18px;
	line-height: 16px;
	width: 100%;
	height: 16px;
}

.aqiDialogNeck .aqiLevel {
	font-size: 13px;
	line-height: 32px;
	width: 100%;
	height: 32px;
}

.aqiDialogNeck .aqiLevel span{
	font-size: 20px;
	font-weight: bold;
	margin-left: 8px;
}

.aqiDialogNeck .primaryPollutant {
	font-size: 13px;
	line-height: 24px;
	width: 100%;
	height: 24px;
}

.aqiDialogNeck .primaryPollutant span {
	font-size: 14px;
	margin-left: 8px;
}

.aqiDialogBody {
	width: 100%;
	height: 208px;
	font-size: 13px;
}

.aqiDialogBody .aqiItemContainer {
	margin: 2px 4px 2px 4px;
	width: 112px;
	height: 64px;
	float: left;
}

.aqiDialogFooter {
	width: 100%;
	height: 68px;
	font-size: 13px;
}

.aqiDialogFooter .title {
	text-align: left;
	padding-left: 16px;
	width: 100%;
	height: 12px;
}

.aqiDialogFooter .aqiLevelContainer {
	width: 100%;
	height: 48px;
	padding: 6px 6px 2px 6px;
}

.aqiDialogFooter .aqiLevelContainer .aqiLevel{
	width: 60px;
	height: 16px;
	text-align: center;
	line-height: 16px;
	margin: 2px 6px 2px 6px;
	font-size: 13px;
	float: left;
}

/* 底部时间轴样式 */
input[type=range] {
	outline: none;
	-webkit-appearance: none;
	-moz-appearance: none;
	background: -webkit-linear-gradient(#8B0000, #8B0000) no-repeat, #A9A9A9;
	background: -moz-linear-gradient(#8B0000, #8B0000) no-repeat, #A9A9A9;
	/*设置左边颜色为#61bd12，右边颜色为#ddd*/
	background-size: 0% 100%; /*设置左右宽度比例*/
	height: 10px; /*横条的高度*/
}

/*拖动块的样式*/
input[type=range]::-webkit-slider-thumb {
	-webkit-appearance: none; /*清除系统默认样式*/
	height: 0px; /*拖动块高度*/
	width: 0px; /*拖动块宽度*/
	background: #61bd12; /*拖动块背景*/
	border-radius: 0%; /*外观设置为圆形*/
	border: solid 0px #ddd; /*设置边框*/
}

table.rangeTooltipTbl tr th{
	border-collapse:collapse;
	border-style:none;
	border-width:1px;
	font-size: 13px;
}

table.rangeTooltipTbl th{
	background-color:rgba(105,105,105,0.5);/* --最后一个参数设置透明度，前面三个是RGB颜色值-- */
	height: 30px;
	width: 70px;
	text-align: left;
	text-indent: 10px;
}

table.rangeTooltipTbl{
	margin-left: -52px;
	margin-top: -7px;
	color: white;
	font-size: 20px;
}

/* 火狐浏览器设置range滑块隐藏 */
input[type=range]::-moz-range-thumb {
	border: 0px solid #000000;
	height: 0px;
	width: 0px;
}

input[type=range]::-moz-range-track {
	height: 0px;
	background: -moz-linear-gradient(#8B0000, #8B0000) no-repeat, #A9A9A9;
	background-size: 0% 100%;
} 
