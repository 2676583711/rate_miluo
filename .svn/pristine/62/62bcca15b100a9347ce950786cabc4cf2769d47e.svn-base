html, body {
	height: 100%;
	width: 100%;
	margin: 0;
	overflow: hidden
}

/* 超级站点位DIV*/
.popupDiv {
	background-color: transparent;
	text-align: center;
	height: 80px;
}

.productItem {
	height: 80px;
	width: 80px;
	background-color: rgba(0, 0, 0, 0.36);
	border-color: #FFFFFF;
	border-style: solid;
	border-width: 0px;
	border-radius: 50%;
	float: left;
	cursor: pointer;
	margin-left: 10px;
	position: relative;
	font-weight:bold;
	/*box-shadow: 5px 5px 10px gray;*/
	color: #FFFFFF;
	font-size: 14px;
	transition: background-color 0.5s, border-color 0.5s;
	-webkit-transition: background-color 0.5s, border-color 0.5s;
}

.produceItemFont {
	margin-top: 24px;
	line-height: 16px;
}

.productItem:hover {
	border-color: #FFFFFF;
	background-color: rgba(216, 202, 47, 0.36);
	/*box-shadow: 10px 10px 20px gray;*/
}

/* 产品弹出窗样式 */
.leaflet-popup-content-wrapper,.leaflet-popup-tip {
	background: transparent !important;;
	box-shadow: none !important;;
}

.popup {
	position: absolute;
	top: -255px;
	left: -263px;
	text-align: center;
	margin-bottom: 20px;
	height: 252px;
	width: 600px;
	z-index: 5;
}
.popup-content-wrapper {
	padding: 10px;
	text-align: left;
	border-radius: 10px;
	background-color: #FFFFFF;
	color: #000000;
	box-shadow: 5px 5px 10px gray;
}
.popup-content {
	line-height: 1.4;
	background-color: #FFFFFF;
}
.popup-content p {
	margin: 18px 0;
}

.popup-tip-container {
	width: 40px;
	height: 20px;
	position: absolute;
	left: 50%;
	margin-left: -20px;
	overflow: hidden;
	pointer-events: none;
}
.popup-tip {
	background-color: #FFFFFF;
	width: 17px;
	height: 17px;
	padding: 1px;
	margin: -10px auto 0;
	box-shadow: 5px 5px 10px gray;
	-webkit-transform: rotate(45deg);
	-moz-transform: rotate(45deg);
	-ms-transform: rotate(45deg);
	-o-transform: rotate(45deg);
	transform: rotate(45deg);
}

/* 右下角图层控件样式 */
#overlaysDiv {
	position: absolute;
	border: rgba(255, 255, 255, 0.5);
	border-style: solid;
	border-width: 1px;
	border-radius: 8px;
	background: rgba(255, 255, 255, 0.2);
	/*margin-top: 60px;*/
	height: 162px;
	width: 102px;
	right: 80px;
	bottom: 32px;
	/*float: left;*/
}

.overlays {
	list-style-type: none;
	margin-top: 10px;
	padding: 0;
}
.overlays li {
	width: 100px;
	height: 30px;
	text-align: center;
	font-size: 12px;
	color: #FFFFFF;
	line-height: 30px;
	background-color: transparent;
	position: relative;
	font-family:Microsoft YaHei,tahoma,serif,simsun,fantasy
}
.overlays li.unchecked {
	background-color: transparent;
}
.overlays li.checked {
	background-color: #9D0300;
}
.overlays li label {
	cursor: default;
	padding: 5px 24px 5px 24px;
}
.overlays span {
	font-size: 12px;
}

#levelDiv {
	border: rgba(255, 255, 255, 0.5);
	border-style: solid;
	border-width: 1px;
	border-radius: 8px;
	background: rgba(255, 255, 255, 0.2);
	height: 240px;
	width: 72px;
	float: left;
	margin-left: 10px;
}

.level {
	list-style-type: none;
	margin-top: 7px;
	padding: 0;
}
.level li {
	width: 70px;
	height: 15px;
	text-align: center;
	font-size: 10px;
	color: #FFFFFF;
	line-height: 15px;
	background-color: transparent;
	position: relative;
	font-family:Microsoft YaHei,tahoma,serif,simsun,fantasy
}
.level li.unchecked {
	background-color: transparent;
}
.level li.checked {
	background-color: #9D0300;
}
.level li label {
	cursor: default;
	padding: 2px 1px 2px 1px;
}

/* 左下角AQI控件样式 */
.aqiContainer {
	height: 20px;
	width: 100%;
	position: relative;
	border: transparent;
	background: transparent;
}

.aqiSlideBtn {
	position: relative;
	left: 50%;
	background-color: #32B8FC;
	width: 60px;
	height: 20px;
	margin-left: -30px;
	cursor: pointer;
	line-height: 18px;
	text-align: center;
}

.arrow-font {
	height: 18px;
	width: 38px;
	font-weight: bold;
}

.aqiContext {
	background-color: #FFFFFF;
	width: 100%;
	height: 591px;
	display: none;
}

/* 右上角AQI项目控件样式 */
.aqiItemDiv {
	border: rgba(255, 255, 255, 0.5);
	border-style: solid;
	border-width: 1px;
	border-radius: 8px;
	background: rgba(255, 255, 255, 0.2);
	margin-top: 5px;
	margin-left: 0px;
	height: 180px;
	width: 52px;
	float: left;
}

.aqiItemOverlays {
	list-style-type: none;
	margin-top: 5px;
	padding: 0;
}
.aqiItemOverlays li {
	width: 40px;
	height: 25px;
	text-align: center;
	line-height: 25px;
	background-color: transparent;
	position: relative;
	font-family:Microsoft YaHei,tahoma,serif,simsun,fantasy
}
.aqiItemOverlays li.unchecked {
	background-color: transparent;
}
.aqiItemOverlays li.checked {
	background-color: #9D0300;
}
.aqiItemOverlays li label {
	font-size: 12px;
	cursor: default;
}

/* AQI详细显示框 */
table.aqiDetailTable {
	width: 100%;
	border: 0px solid #d7d7d7;
	background-color: #fff;
	font-size: 12px;
}

table.aqiDetailTable tr td {
	border-bottom: 1px solid #d7d7d7;
	border-right: 1px solid #d7d7d7;
	text-align: left;
	height: 18px;
	line-height: 18px;
}

/* 左上角AQI详细窗口控件 */
#aqiDialogDiv {
	height: 100%;
	width: 242px;
	min-height: 410px;
	position: relative;
	border: transparent;
	background: transparent;
}

.aqiDialogDiv {
	position: relative;
	margin-top: 40px;
