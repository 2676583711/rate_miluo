package com.rate.web.connected.offdata.entity;


import org.beetl.sql.core.annotatoin.Table;

import java.util.Date;


@Table(name = "miluo_statement_type")
public class MiluoStatementType {
    //设备编码
    private String equipmentId ;
    private String id ;
    //在线状态(0:在线，1:离线)
    private String siteStatement ;
    //11:水质   21:微型站   22:标准站  23:国控站  31:涉水污染源
    private String siteType ;
    //时间
    private Date time ;

    private String name;
    private String  videoName;

    public String getVideoName() {
        return videoName;
    }

    public void setVideoName(String videoName) {
        this.videoName = videoName;
    }

    private int lx;

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public int getLx() {
        return lx;
    }

    public void setLx(int lx) {
        this.lx = lx;
    }

    private String idList;

    private String maxOnLine;

    private String  offCount;

    public String getIdList() {
        return idList;
    }

    public void setIdList(String idList) {
        this.idList = idList;
    }


    public String getMaxOnLine() {
        return maxOnLine;
    }

    public void setMaxOnLine(String maxOnLine) {
        this.maxOnLine = maxOnLine;
    }

    public String getOffCount() {
        return offCount;
    }

    public void setOffCount(String offCount) {
        this.offCount = offCount;
    }


    public MiluoStatementType() {
    }

    /**设备编码
     *@return
     */
    public String getEquipmentId(){
        return  equipmentId;
    }
    /**设备编码
     *@param  equipmentId
     */
    public void setEquipmentId(String equipmentId ){
        this.equipmentId = equipmentId;
    }

    public String getId(){
        return  id;
    }
    public void setId(String id ){
        this.id = id;
    }

    /**在线状态(0:在线，1:离线)
     *@return
     */
    public String getSiteStatement(){
        return  siteStatement;
    }
    /**在线状态(0:在线，1:离线)
     *@param  siteStatement
     */
    public void setSiteStatement(String siteStatement ){
        this.siteStatement = siteStatement;
    }

    /**11:水质   21:微型站   22:标准站  23:国控站  31:涉水污染源
     *@return
     */
    public String getSiteType(){
        return  siteType;
    }
    /**11:水质   21:微型站   22:标准站  23:国控站  31:涉水污染源
     *@param  siteType
     */
    public void setSiteType(String siteType ){
        this.siteType = siteType;
    }

    /**时间
     *@return
     */
    public Date getTime(){
        return  time;
    }
    /**时间
     *@param  time
     */
    public void setTime(Date time ){
        this.time = time;
    }

}
