.p1 {
    pointer-events: none;
    position: absolute;
    /*top: -24px;*/
    /*left: -16px;*/
    top: -15px;
    left: -13px;
    transform: rotate(65deg);
    animation: an1 2s infinite linear;
    -webkit-animation: an1 2s infinite linear;
}

.p1 img {
    width: 36px;
    height: 36px;
}

.p2 img {
    width: 10px;
    height: 14px;
}

img {
    border: none;
}

/*.test img {*/
/*width: 16px;*/
/*height: 24px;*/
/*}*/

@keyframes an1 {
    0% {
        transform: rotate(0deg);
        opacity: 0.5;
    }

    100% {
        transform: rotate(360deg);
        opacity: 0.5;
    }
}

@-webkit-keyframes an1 {
    0% {
        transform: rotate(0deg);
        opacity: 0.5;
    }

    100% {
        transform: rotate(360deg);
        opacity: 0.5;
    }
}

@-moz-keyframes an1 {
    0% {
        transform: rotate(0deg);
        opacity: 0.5;
    }

    100% {
        transform: rotate(360deg);
        opacity: 0.5;
    }
}

@-o-keyframes an1 {
    0% {
        transform: rotate(0deg);
        opacity: 0.5;
    }

    100% {
        transform: rotate(360deg);
        opacity: 0.5;
    }
}

