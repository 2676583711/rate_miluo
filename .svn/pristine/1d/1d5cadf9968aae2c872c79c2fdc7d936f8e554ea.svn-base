@keyframes plusNone {
    0% {
        width: 50px;
        height: 50px;
    }
    100% {
        width: 135px;
        height: 135px;
    }
}

@keyframes plus {
    0% {
        width: 110px;
        height: 110px;
    }
    100% {
        width: 135px;
        height: 135px;
    }
}

@keyframes reduce {
    0% {
        width: 135px;
        height: 135px;
    }
    100% {
        width: 110px;
        height: 110px;
    }
}

@keyframes reduceNone {
    0% {
        width: 110px;
        height: 110px;
    }
    100% {
        width: 50px;
        height: 50px;
    }
}

@keyframes plusActive {
    0% {
        width: 110px;
        height: 110px;
    }
    100% {
        width: 135px;
        height: 135px;
    }
}

