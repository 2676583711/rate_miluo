ion.\n */\n\n\nexport class BasePage {\n  constructor(\n    public navCtrl: NavController,\n    public navParams: NavParams,\n    public toastCtrl?: ToastController) {\n  }\n\n  toast(info) {\n    if (this.toastCtrl != null)\n      this.toastCtrl.create({\n        message: info,\n        duration: 3000,\n        position: 'middle',\n        showCloseButton: true,\n        closeButtonText: \"关闭\"\n      }).present();\n  }\n\n  toastLong(info) {\n    if (this.toastCtrl != null)\n      this.toastCtrl.create({\n        message: info,\n        duration: 3000,\n        position: 'middle',\n        showCloseButton: true,\n        closeButtonText: \"关闭\"\n      }).present();\n  }\n\n  toastShort(info) {\n    if (this.toastCtrl != null)\n      this.toastCtrl.create({\n        message: info,\n        duration: 1500,\n        position: 'middle',\n        showCloseButton: true,\n        closeButtonText: \"关闭\"\n      }).present();\n  }\n}\n\n\n// WEBPACK FOOTER //\n// ./src/pages/base/base.ts","import { AppGlobal } from './../../providers/app-service/app-service';\nimport { Component, ViewChild } from '@angular/core';\nimport { IonicPage, NavController, NavParams, Scroll, Content, ToastController, MenuController } from 'ionic-angular';\nimport { BasePage } from '../base/base';\nimport { NetworkServiceProvider } from '../../providers/network-service/network-service';\nimport { AppServiceProvider } from '../../providers/app-service/app-service';\n\n/**\n * Generated class for the WamingDetailsPage page.\n *\n * See https://ionicframework.com/docs/components/#navigation for more info on\n * Ionic pages and navigation.\n */\nlet wamingThis;\ndeclare const $;\n@IonicPage()\n@Component({\n  selector: 'page-waming-details',\n  templateUrl: 'waming-details.html',\n})\nexport class WamingDetailsPage extends BasePage {\n  @ViewChild(\"wamingContent\") wamingContent: Content;\n  @ViewChild(\"headerWaming\") headerWaming: Scroll;\n  @ViewChild(\"leftWaming\") leftWaming: Scroll;\n  @ViewChild(\"rightWaming\") rightWaming: Scroll;\n  siteType: any = '2';\n  status: any = 0;\n  treeId: any = '';\n  trees: any = '';\n  startTime: any = '';\n  endTime: any = '';\n  pageNum: any = 0;\n  pageSize: any = 30;\n  listAlert: any[] = [];\n  style: style = new style;\n  wamingHeight: any;\n  role: any;\n  siteSelection: boolean;\n\n  constructor(public navCtrl: NavController,\n    public navParams: NavParams,\n    public taostCtrl: ToastController,\n    public net: NetworkServiceProvider,\n    public menuCtrl: MenuController) {\n    super(navCtrl, navParams, taostCtrl);\n    wamingThis = this;\n  }\n  statuss: any = [\n    { id: 0, name: \"未处理\" },\n    { id: 1, name: \"已处理\" },\n  ];\n  siteTypes: any = [\n    { id: '2', name: \"空气站\" },\n    { id: '3', name: \"污染源站\" },\n    { id: '1', name: \"水质自动站\" },\n    { id: '4', name: \"污水处理厂\" },\n  ];\n  ionViewDidLoad() {\n    console.log('ionViewDidLoad WamingDetailsPage');\n    this.siteSelections();\n    this.time();\n    this.getListAlert();\n    this.scroll();\n    this.getSiteTree();\n  }\n  siteSelections() {\n    this.role = AppServiceProvider.getInstance().userInfo.role[0];\n    console.log(\"role :  \" + this.role);\n    if (this.role == '1' || this.role == '77') {\n      this.siteSelection = true;\n      this.siteType = '2';\n    } else {\n      if (this.role == '78') {\n        this.siteType = '1';\n      } else if (this.role == '79') {\n        this.siteType = '3'\n      } else if (this.role == '81') {\n        this.siteType = '2'\n      } else if (this.role = '82') {\n        this.siteType = '4'\n      }\n      this.siteSelection = false;\n    }\n    console.error\n      (this.siteSelection)\n  }\n  query() {\n    this.menuCtrl.enable(true, \"wamingDetailsMenu\");\n    this.menuCtrl.toggle(\"wamingDetailsMenu\");\n  }\n  time() {\n    let time = new Date;\n    console.log(time);\n    let fullYear = time.getFullYear().toString();\n    let month = time.getMonth();\n    let date = time.getDate();\n    let hour = time.getHours();\n    let minute = time.getMinutes();\n    let months;\n    let dates;\n    let hours;\n    let minutes;\n    month = month + 1;\n    tiem();\n    this.endTime = fullYear + '-' + months + '-' + dates + 'T' + hours + \":\" + minutes;\n    date = date - 1;\n    tiem();\n    this.startTime = fullYear + '-' + months + '-' + dates + 'T' + hours + \":\" + minutes;\n\n    function tiem() {\n      if (month.toString().length == 1) {\n        months = '0' + month.toString();\n      } else {\n        months = month.toString();\n      }\n      if (date.toString().length == 1) {\n        dates = '0' + date.toString();\n      } else {\n        dates = date.toString();\n      }\n      if (hour.toString().length == 1) {\n        hours = '0' + hour.toString();\n      } else {\n        hours = hour.toString();\n      }\n      if (minute.toString().length == 1) {\n        minutes = '0' + minute.toString();\n      } else {\n        minutes = minute.toString();\n      }\n      console.log(fullYear + '-' + months + '-' + dates + 'T' + hours + \":\" + minutes)\n    }\n  }\n  determine() {\n    this.listAlert = [];\n    this.getListAlert();\n    this.menuCtrl.enable(true, \"wamingDetailsMenu\");\n    this.menuCtrl.toggle(\"wamingDetailsMenu\");\n  }\n  getSiteTree() {\n    let param: any = {};\n    param.userId = AppServiceProvider.getInstance().userInfo.userId;\n    param.code = AppServiceProvider.getInstance().userInfo.code;\n    this.net.httpPost(\n      AppGlobal.API.getSiteTree,\n      param, res => {\n        console.log(res);\n        this.trees = res;\n        this.tree();\n      }, error => {\n        this.toast(error);\n      }\n    )\n  }\n  getStyle(name) {\n    var arr = [];\n    let arrs = null;\n    let newArr: number[] = null;\n    let names = document.getElementsByName(name);\n    for (var i = 0; i < names.length; i++) {\n      arr.push(document.getElementsByName(name)[i].offsetWidth);\n    }\n    arrs = new Set(arr);\n    newArr = Array.from(arrs);\n    console.log(name)\n    console.log(names.length)\n    console.log(arrs)\n    return Math.max.apply(null, newArr);\n  }\n  scrollStyle() {\n    let wamingContent = $(\"#wamingContent\").height();\n    let wamingHeader = $(\"#wamingHeader\").height();\n    let wamingHead = $(\"#wamingHead\").height();\n    console.log(\"----------------------\")\n    this.style.wamingName = this.getStyle(\"wamingName\") + 10 + \"px\";\n    this.style.wamingPollutant = this.getStyle(\"wamingPollutant\") + \"px\";\n    this.style.wamingValue = this.getStyle(\"wamingValue\") + \"px\";\n    this.style.wamingExceedsBidDate = this.getStyle(\"wamingExceedsBidDate\") + \"px\";\n    this.style.wamingStatus = this.getStyle(\"wamingStatus\") + \"px\";\n    let wamingStationNames = $(\"#wamingStationNames\").height();\n    let wamingHeight = wamingContent - wamingHeader - wamingHead - 30;\n    if (wamingStationNames < wamingHeight) {\n      this.style.wamingHeight = wamingStationNames + 'px';\n    } else {\n      this.style.wamingHeight = wamingHeight + 'px';\n    }\n  }\n  scroll() {\n    this.rightWaming.addScrollEventListener(function (event: any) {\n      wamingThis.headerWaming._scrollContent.nativeElement.scrollLeft = event.target.scrollLeft;\n      wamingThis.leftWaming._scrollContent.nativeElement.scrollTop = event.target.scrollTop;\n    });\n  }\n  tree() {\n    console.log(this.siteType)\n    var treeHtml = document.getElementById(\"treeHtml\");\n    var jsTreeWaming = document.getElementById(\"jsTreeWaming\");\n    treeHtml.removeChild(jsTreeWaming);\n    treeHtml.innerHTML = \"<div id='jsTreeWaming' class='test-1'></div> \";\n\n    let type;\n    if (this.siteType == '2') {\n      type = \"空气站\"\n    } else if (this.siteType == '3') {\n      type = \"污染源站\"\n    } else if (this.siteType == '1') {\n      type = \"水质站\"\n    } else if (this.siteType == '4') {\n      type = \"污水处理厂\"\n    }\n    this.treeId = ''\n    // //document.getElementById(\"jstreeWaming\").removeChild\n    // var elem = document.getElementById('jstreeWaming'); // 按 id 获取要删除的元素\n    // elem.parentNode.removeChild(elem)\n    $('#jsTreeWaming').jstree({\n      'core': {\n        \"themes\": {\n          \"dots\": false // no connecting dots between dots\n        },\n        'data': this.trees[type]\n      },\n      \"plugins\": [\"search\"]\n    });\n    $('#jsTreeWaming').jstree().open_all();\n    $('#jsTreeWaming').on(\"changed.jstree\", (event, data) => {\n      var str = data.node.id;\n\n      if (str.slice(0, 1) != \"-\") {\n        this.treeId = str;\n      } else {\n        this.treeId = '';\n      }\n      console.log(this.treeId)\n    });\n  }\n  getListAlert() {\n    let param: any = {};\n    param.siteType = this.siteType;\n    param.siteId = this.treeId;\n    param.startTime = this.startTime.replace(\"T\", \" \");\n    param.endTime = this.endTime.replace(\"T\", \" \");\n    param.status = this.status;\n    param.pageNum = this.pageNum;\n    param.pageSize = this.pageSize;\n    param.userId = AppServiceProvider.getInstance().userInfo.userId;\n    param.code = AppServiceProvider.getInstance().userInfo.code;\n    this.net.httpPost(\n      AppGlobal.API.getListAlert,\n      param, res => {\n        console.log(res);\n        for (let listAlert of res) {\n          this.listAlert.push(listAlert);\n        }\n        console.log(this.listAlert);\n        setTimeout(() => {\n          this.scrollStyle();\n        }, 500)\n      }, error => {\n        this.toast(error);\n      }\n    )\n  }\n  loadMore() {\n    this.pageNum = this.listAlert.length;\n    this.getListAlert();\n  }\n  clickTaskAdd(value) {\n    this.navCtrl.push(\"TaskAddPage\", { waming: value });\n  }\n}\nclass style {\n  wamingName: string = '';\n  wamingPollutant: string = '';\n  wamingValue: string = '';\n  wamingExceedsBidDate: string = '';\n  wamingStatus: string = '';\n  wamingHeight: string = '';\n}\n\n\n\n// WEBPACK FOOTER //\n// ./src/pages/waming-details/waming-details.ts"],"sourceRoot":""}