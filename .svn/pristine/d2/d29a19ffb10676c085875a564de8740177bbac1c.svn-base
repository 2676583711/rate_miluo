c;OARlB,YAAY,CA8DxB;8DAAD,GAAC;AAAA;SA9DY,YAAY,gB","file":"9.js","sourcesContent":["import { NgModule } from '@angular/core';\nimport { IonicPageModule } from 'ionic-angular';\nimport { TaskEditPage } from './task-edit';\n\n@NgModule({\n  declarations: [\n    TaskEditPage,\n  ],\n  imports: [\n    IonicPageModule.forChild(TaskEditPage),\n  ],\n})\nexport class TaskEditPageModule {}\n\n\n\n// WEBPACK FOOTER //\n// ./src/pages/task-edit/task-edit.module.ts","import { NavController, NavParams, ToastController } from 'ionic-angular';\n\n/**\n * Generated class for the BasePage page.\n *\n * See https://ionicframework.com/docs/components/#navigation for more info on\n * Ionic pages and navigation.\n */\n\n\nexport class BasePage {\n  constructor(\n    public navCtrl: NavController,\n    public navParams: NavParams,\n    public toastCtrl?: ToastController) {\n  }\n\n  toast(info) {\n    if (this.toastCtrl != null)\n      this.toastCtrl.create({\n        message: info,\n        duration: 3000,\n        position: 'middle',\n        showCloseButton: true,\n        closeButtonText: \"关闭\"\n      }).present();\n  }\n\n  toastLong(info) {\n    if (this.toastCtrl != null)\n      this.toastCtrl.create({\n        message: info,\n        duration: 3000,\n        position: 'middle',\n        showCloseButton: true,\n        closeButtonText: \"关闭\"\n      }).present();\n  }\n\n  toastShort(info) {\n    if (this.toastCtrl != null)\n      this.toastCtrl.create({\n        message: info,\n        duration: 1500,\n        position: 'middle',\n        showCloseButton: true,\n        closeButtonText: \"关闭\"\n      }).present();\n  }\n}\n\n\n// WEBPACK FOOTER //\n// ./src/pages/base/base.ts","import { ViewController } from 'ionic-angular/navigation/view-controller';\nimport { NetworkServiceProvider } from './../../providers/network-service/network-service';\nimport { BasePage } from './../base/base';\nimport { Component } from '@angular/core';\nimport { IonicPage, NavController, NavParams, ToastController } from 'ionic-angular';\nimport { AppServiceProvider, AppGlobal } from '../../providers/app-service/app-service';\n\n/**\n * Generated class for the TaskEditPage page.\n *\n * See https://ionicframework.com/docs/components/#navigation for more info on\n * Ionic pages and navigation.\n */\n\n@IonicPage()\n@Component({\n  selector: 'page-task-edit',\n  templateUrl: 'task-edit.html',\n})\nexport class TaskEditPage extends BasePage {\n  date: any;\n  taskInfo: any;\n\n  constructor(public navCtrl: NavController,\n    public navParams: NavParams,\n    public toastCtrl: ToastController,\n    public net: NetworkServiceProvider,\n    public view: ViewController) {\n    super(navCtrl, navParams, toastCtrl);\n    this.date = this.navParams.data.data;\n    this.taskInfo = this.date;\n    this.taskInfo.startTime = this.taskInfo.startTime.replace(\" \", \"T\");\n    this.taskInfo.endTime = this.taskInfo.endTime.replace(\" \", \"T\");\n    console.log(this.taskInfo)\n\n  }\n  taskTypes: any = [\n    { id: '0', name: \"报警任务\" },\n    { id: '1', name: \"巡检\" },\n    { id: '2', name: \"维护\" },\n    { id: '3', name: \"易耗品更换\" },\n    { id: '4', name: \"仪器校准\" },\n    { id: '5', name: \"维修\" },\n    { id: '6', name: \"试剂更换\" },\n    { id: '7', name: \"废液回收\" },\n    { id: '8', name: \"其他\" },\n  ];\n  degreeEmergencys: any = [\n    { id: '0', name: \"普通不紧急\" },\n    { id: '1', name: \"重要紧急\" },\n    { id: '2', name: \"最高级别\" },\n  ];\n  statusList: any = [\n    { id: '0', name: \"未处理\" },\n    { id: '1', name: \"已处理\" },\n  ];\n  ionViewDidLoad() {\n    console.log('ionViewDidLoad TaskEditPage');\n  }\n  return(){\n    this.view.dismiss(this.date)\n  }\n  determine() {\n    let param: any = {};\n    this.taskInfo.startTime = this.taskInfo.startTime.replace(\"T\", \" \");\n    this.taskInfo.endTime = this.taskInfo.endTime.replace(\"T\", \" \");\n    param.userId = AppServiceProvider.getInstance().userInfo.userId;\n    param.code = AppServiceProvider.getInstance().userInfo.code;\n    for (let key in this.taskInfo) {\n      param[key] = this.taskInfo[key];\n    }\n    console.log(param)\n    this.net.httpPost(\n      AppGlobal.API.updateTaskById, param, res => {\n        this.toast(\"编辑成功\");\n        this.view.dismiss(this.taskInfo)\n      }, error => {\n        this.toast(error)\n      }\n    )\n  }\n}\n\n\n\n// WEBPACK FOOTER //\n// ./src/pages/task-edit/task-edit.ts"],"sourceRoot":""}