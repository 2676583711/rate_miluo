s and navigation.\n */\n\n\nexport class BasePage {\n  constructor(\n    public navCtrl: NavController,\n    public navParams: NavParams,\n    public toastCtrl?: ToastController) {\n  }\n\n  toast(info) {\n    if (this.toastCtrl != null)\n      this.toastCtrl.create({\n        message: info,\n        duration: 3000,\n        position: 'middle',\n        showCloseButton: true,\n        closeButtonText: \"关闭\"\n      }).present();\n  }\n\n  toastLong(info) {\n    if (this.toastCtrl != null)\n      this.toastCtrl.create({\n        message: info,\n        duration: 3000,\n        position: 'middle',\n        showCloseButton: true,\n        closeButtonText: \"关闭\"\n      }).present();\n  }\n\n  toastShort(info) {\n    if (this.toastCtrl != null)\n      this.toastCtrl.create({\n        message: info,\n        duration: 1500,\n        position: 'middle',\n        showCloseButton: true,\n        closeButtonText: \"关闭\"\n      }).present();\n  }\n}\n\n\n// WEBPACK FOOTER //\n// ./src/pages/base/base.ts","import { BasePage } from './../base/base';\nimport { Component, ViewChild } from '@angular/core';\nimport { IonicPage, NavController, NavParams, ToastController, MenuController, Content, Scroll } from 'ionic-angular';\nimport { NetworkServiceProvider } from '../../providers/network-service/network-service';\nimport { AppServiceProvider, AppGlobal } from '../../providers/app-service/app-service';\n\n/**\n * Generated class for the ContaminatedMonitorPage page.\n *\n * See https://ionicframework.com/docs/components/#navigation for more info on\n * Ionic pages and navigation.\n */\nlet contaminatedThis;\ndeclare const $;\n@IonicPage()\n@Component({\n  selector: 'page-contaminated-monitor',\n  templateUrl: 'contaminated-monitor.html',\n})\nexport class ContaminatedMonitorPage extends BasePage {\n  @ViewChild(\"contaminatedContent\") contaminatedContent: Content;\n  @ViewChild(\"headerContaminated\") headerContaminated: Scroll;\n  @ViewChild(\"leftContaminated\") leftContaminated: Scroll;\n  @ViewChild(\"rightContaminated\") rightContaminated: Scroll;\n  arrDate: string = \"minute\";\n  minuteDate: minuteDate = new minuteDate;\n  contaminatedData: any = [];\n  trees: any;\n  treeId: any = '';\n  dataType: any = '1';\n  pageNum: any = 0;\n  pageSize: any = 30;\n  style: style = new style;\n  displayFormat: any;\n  constructor(public navCtrl: NavController,\n    public navParams: NavParams,\n    public toastCtrl: ToastController,\n    public menuCtrl: MenuController,\n    public net: NetworkServiceProvider) {\n    super(navCtrl, navParams, toastCtrl)\n    contaminatedThis = this;\n  }\n\n  ionViewDidLoad() {\n    console.log('ionViewDidLoad ContaminatedMonitorPage');\n    this.time();\n    this.scroll();\n    this.getContaminatedData();\n    this.getSiteTree();\n  }\n  query() {\n    this.menuCtrl.enable(true, \"contaminatedMenu\");\n    this.menuCtrl.toggle(\"contaminatedMenu\");\n  }\n  determine() {\n    this.contaminatedData = [];\n    this.getContaminatedData();\n    this.menuCtrl.enable(true, \"contaminatedMenu\");\n    this.menuCtrl.toggle(\"contaminatedMenu\");\n  }\n  time() {\n    if (this.arrDate == \"minute\") {\n      this.displayFormat = \"YYYY-MM-DD hh:mm\";\n      this.minuteDate.startDate = new Date(new Date().getTime() + 7 * 60 * 60 * 1000).toISOString();\n      this.minuteDate.endDate = new Date(new Date().getTime() + 8 * 60 * 60 * 1000).toISOString();\n    } else if (this.arrDate == \"hour\") {\n      this.displayFormat = \"YYYY-MM-DD hh\";\n      this.minuteDate.startDate = new Date(new Date().getTime() + 5 * 60 * 60 * 1000).toISOString();\n      this.minuteDate.endDate = new Date(new Date().getTime() + 8 * 60 * 60 * 1000).toISOString();\n    } else if (this.arrDate == \"day\") {\n      this.displayFormat = \"YYYY-MM-DD\";\n      this.minuteDate.startDate = new Date(new Date().getTime() - 4 * 16 * 60 * 60 * 1000).toISOString();\n      this.minuteDate.endDate = new Date(new Date().getTime() + 8 * 60 * 60 * 1000).toISOString();\n    }\n  }\n  getSiteTree() {\n    let param: any = {};\n    param.userId = AppServiceProvider.getInstance().userInfo.userId;\n    param.code = AppServiceProvider.getInstance().userInfo.code;\n    this.net.httpPost(\n      AppGlobal.API.getSiteTree,\n      param, res => {\n        this.trees = res;\n        console.log(this.trees);\n        this.tree();\n      }, error => {\n        this.toast(error);\n      }\n    )\n  }\n\n  tree() {\n    $('#jsTreeContaminated').jstree({\n      'core': {\n        \"themes\": {\n          \"dots\": false // no connecting dots between dots\n        },\n        'data': this.trees[\"涉气污染源\"]\n      },\n      \"plugins\": [\"search\"]\n    });\n    $('#jsTreeContaminated').jstree().open_all();\n    $('#jsTreeContaminated').on(\"changed.jstree\", (event, data) => {\n      var str = data.node.id;\n\n      if (str.slice(0, 1) != \"-\") {\n        this.treeId = str;\n      } else {\n        this.treeId = '';\n      }\n      console.log(this.treeId)\n    });\n  }\n  scrollStyle() {\n    let contaminatedContent = $(\"#contaminatedContent\").height();\n    let contaminatedHeader = $(\"#contaminatedHeader\").height();\n    let contaminatedHead = $(\"#contaminatedHead\").height();\n    let contaminatedStationNames = $(\"#contaminatedStationNames\").height();\n    let contaminatedHeight = contaminatedContent - contaminatedHeader - contaminatedHead - 30;\n    if (contaminatedStationNames < contaminatedHeight) {\n      this.style.contaminatedHeight = contaminatedStationNames + 'px';\n    } else {\n      this.style.contaminatedHeight = contaminatedContent - contaminatedHeader - contaminatedHead - 30 + 'px';\n    }\n  }\n  scroll() {\n    this.rightContaminated.addScrollEventListener(function (event: any) {\n      contaminatedThis.headerContaminated._scrollContent.nativeElement.scrollLeft = event.target.scrollLeft;\n      contaminatedThis.leftContaminated._scrollContent.nativeElement.scrollTop = event.target.scrollTop;\n    });\n  }\n\n  getContaminatedData() {\n    let startDate;\n    let endDate;\n    if (this.arrDate == \"minute\") {\n      this.dataType = '1';\n      startDate = this.minuteDate.startDate.replace(\"T\", \" \").replace(\"Z\", \"\").substring(0, 16) + ':00';\n      endDate = this.minuteDate.endDate.replace(\"T\", \" \").replace(\"Z\", \"\").substring(0, 16) + ':00';\n    } else if (this.arrDate == \"hour\") {\n      startDate = this.minuteDate.startDate.replace(\"T\", \" \").replace(\"Z\", \"\").substring(0, 13) + ':00:00';\n      endDate = this.minuteDate.endDate.replace(\"T\", \" \").replace(\"Z\", \"\").substring(0, 13) + ':00:00';\n      this.dataType = '2';\n    } else if (this.arrDate == \"day\") {\n      startDate = this.minuteDate.startDate.replace(\"T\", \" \").replace(\"Z\", \"\").substring(0, 10) + ' 00:00:00';\n      endDate = this.minuteDate.endDate.replace(\"T\", \" \").replace(\"Z\", \"\").substring(0, 10) + ' 00:00:00';\n      this.dataType = '3';\n    }\n    let param: any = {};\n    param.siteId = this.treeId;\n    param.startTime = startDate;\n    param.endTime = endDate;\n    param.dataType = this.dataType;\n    param.pageNum = this.pageNum;\n    param.pageSize = this.pageSize;\n    param.userId = AppServiceProvider.getInstance().userInfo.userId;\n    param.code = AppServiceProvider.getInstance().userInfo.code;\n    this.net.httpPost(\n      AppGlobal.API.getPolluteData,\n      param, res => {\n        if (res) {\n          for (let contaminatedData of res) {\n            this.contaminatedData.push(contaminatedData);\n          }\n        }\n\n        console.log(this.contaminatedData);\n        setTimeout(() => {\n          this.scrollStyle();\n        }, 10)\n      }, error => {\n        this.toast(error);\n      }\n    )\n  }\n  loadMore() {\n    this.pageNum = this.contaminatedData.length;\n    this.getContaminatedData();\n  }\n  monitor(item){\n    item.siteType='3';\n    this.navCtrl.push('ContaminatedDetailsPage', { site: item, id: '1' });\n  }\n}\nclass minuteDate {\n  maxDate: string = \"\";\n  startDate: string = \"\";\n  endDate: string = \"\";\n}\nclass style {\n  contaminatedSiteName: string = '';\n  contaminatedEquipmentName: string = '';\n  contaminatedRecordingTime: string = '';\n  contaminatedBo1: string = '';\n  contaminatedPh: string = '';\n  contaminatedPb: string = '';\n  contaminatedCd: string = '';\n  contaminatedShen: string = '';\n  contaminatedZn: string = '';\n  contaminatedCu: string = '';\n  contaminatedHeight: string = '';\n}\n\n\n\n// WEBPACK FOOTER //\n// ./src/pages/contaminated-monitor/contaminated-monitor.ts"],"sourceRoot":""}